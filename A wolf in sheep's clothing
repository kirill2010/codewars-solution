function warnTheSheep(arr) {
  arr = arr.reverse()
let wolf = arr.indexOf('wolf')
console.log(arr)
 if(wolf === 0){
  return 'Pls go away and stop eating my sheep'
 }else{
  return `Oi! Sheep number ${wolf}! You are about to be eaten by a wolf!` 
 }
}
